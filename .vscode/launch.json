    "version": "0.2.0",
    "configurations": [

        {
            "name": "debug dev",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "runserver",
                "--settings=core.dev_settings"
            ],
            "django": true,
            "autoStartBrowser": false,
            "program": "${workspaceFolder}\\manage.py"
        },
        {
            "name": "makemigrations dev",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "makemigrations",
                "--settings=core.dev_settings"
            ],
            "django": true,
            "autoStartBrowser": false,
            "program": "${workspaceFolder}\\manage.py"
        },
        {
            "name": "migrate dev",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "migrate",
                "--settings=core.dev_settings"
            ],
            "django": true,
            "autoStartBrowser": false,
            "program": "${workspaceFolder}\\manage.py"
        }
    ]
}
